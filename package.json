{
  "name": "generator-ca",
  "version": "0.0.0-development",
  "description": "Yeomangenerator to saffold common JavaScript modules and components. This library is meant to be used to generate all CA projects and components.",
  "homepage": "https://github.com/CAAPIM/generator-ca",
  "author": {
    "name": "CA Technologies",
    "email": "github@ca.com",
    "url": "https://www.ca.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "ca",
    "react",
    "component"
  ],
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-ca": "^2.0.0",
    "jest": "^19.0.2",
    "rimraf": "^2.6.1",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.6.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "codecov": "^2.1.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.3",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "parse-github-url": "^1.0.0",
    "semantic-release": "^6.3.2",
    "validate-commit-msg": "^2.12.0",
    "yeoman-generator": "^1.0.0",
    "yosay": "^2.0.0"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage/",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverageFrom": [
      "generators/**/*.js",
      "!generators/*/templates/**/*",
      "!tests/acceptance"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/generators/component/templates/",
      "<rootDir>/tests/acceptance/"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "test:acceptance": "node ./tests/acceptance/component.js",
    "test:coverage": "jest --coverage",
    "codecov": "codecov",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run lint && npm run test",
    "release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CAAPIM/generator-ca.git"
  },
  "license": "MIT"
}
